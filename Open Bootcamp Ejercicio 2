// Clase coche
class Coche {
  constructor(){
    this.numPuertas = 0;
  }
  
  incrementarPuertas(){
    this.numPuertas++;
  }
}

// Objeto miCoche
const miCoche = new Coche();

// Añadir una puerta
miCoche.incrementarPuertas();

// Mostrar número de puertas
console.log(`El coche tiene ${miCoche.numPuertas} puertas.`);
